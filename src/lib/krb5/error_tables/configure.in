AC_SUBST(BOGUS)
SubdirLibraryRule([${OBJS}])
CopyHeader(isode_err.h,[$(EHDRDIR)])
CopyHeader(krb5_err.h,[$(EHDRDIR)])
CopyHeader(kdb5_err.h,[$(EHDRDIR)])
CopyHeader(adm_err.h,[$(EHDRDIR)])
CopyHeader(asn1_err.h,[$(EHDRDIR)])
V5_AC_OUTPUT_MAKEFILE
