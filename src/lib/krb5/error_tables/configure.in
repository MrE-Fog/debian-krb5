AC_INIT(configure.in)
WITH_CCOPTS
ISODE_DEFS
CONFIG_RULES
AC_SET_BUILDTOP
ET_RULES
SS_RULES
KRB_INCLUDE
AC_ARG_ENABLE([isode],[build and use ISODE to generate ASN.1 stubs],[
ASN1_HDRS=isode_err.h
ASN1_OBJS=isode_err.o
ASN1_SRCS=isode_err.c
BOGUS=nosuchtarget],[
ASN1_HDRS=asn1_err.h
ASN1_OBJS=asn1_err.o
