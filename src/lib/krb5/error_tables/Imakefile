ETSRCS=isode_err.c kdb5_err.c krb5_err.c

ETOBJS=isode_err.o kdb5_err.o krb5_err.o

ETHDRS=isode_err.h kdb5_err.h krb5_err.h

SRCS= $(ETSRCS) init_ets.c
OBJS= $(ETOBJS) init_ets.o
HDRS= $(ETHDRS)

EHDRDIR=$(TOP)/include/krb5

NormalLibraryObjectRule()
ErrorTableObjectRule()

all:: ${SRCS} ${HDRS} includes ${OBJS}

clean::
	$(RM) $(ETHDRS) $(ETSRCS)

CopyHeader(isode_err.h,$(EHDRDIR))
CopyHeader(krb5_err.h,$(EHDRDIR))
CopyHeader(kdb5_err.h,$(EHDRDIR))


OtherdirLibraryTarget($(TOP)/lib,krb5,$(OBJS))

DependTarget()

Krb5InstallHeaders($(ETHDRS), $(KRB5_INCDIR)/krb5)

