PepyPosyRules()
NormalLibraryObjectRule()

SRCS= \
	KRB5-asn.py	\
	asn1glue.c	\
	encode.c 	\
	decode.c

OBJS= \
	KRB5-types.o	\
	asn1glue.o	\
	encode.o	\
	decode.o

OtherdirLibraryTarget($(TOP)/lib,krb5,$(OBJS))

KRB5-types.c: KRB5-types.py

KRB5-types.py:	KRB5-asn.py
	$(POSY) $(POFLAGS) -o $@ KRB5-asn.py

clean::
	$(RM) KRB5-types.c KRB5.ph KRB5-types.py KRB5-types.h
