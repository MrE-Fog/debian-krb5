CFLAGS=$(CCOPTS) $(DEFS)

##DOSBUILDTOP = ..\..\..
##DOSLIBNAME=..\krb5.lib
##DOS!include $(BUILDTOP)\config\windows.in

EHDRDIR=$(BUILDTOP)/include/krb5/asn.1

SRCS= \
	$(srcdir)asn1_decode.c\
	$(srcdir)asn1_k_decode.c\
	$(srcdir)asn1_ncode.c\
	$(srcdir)asn1_get.c\
	$(srcdir)asn1_make.c\
	$(srcdir)asn1buf.c\
	$(srcdir)krb5_decode.c\
	$(srcdir)krb5_encode.c\
	$(srcdir)asn1_k_encode.c\
	$(srcdir)asn1_misc.c

OBJS= \
	asn1_decode.$(OBJEXT)\
	asn1_k_decode.$(OBJEXT)\
	asn1_encode.$(OBJEXT)\
	asn1_get.$(OBJEXT)\
	asn1_make.$(OBJEXT)\
	asn1buf.$(OBJEXT)\
	krb5_decode.$(OBJEXT)\
	krb5_encode.$(OBJEXT)\
	asn1_k_encode.$(OBJEXT)\
	asn1_misc.$(OBJEXT)

all:: $(OBJS)

clean:: clean-$(WHAT)

clean-unix::
	$(RM) $(OBJS)

clean-mac::
	$(RM) $(OBJS)

clean-windows::
	$(RM) *.obj
