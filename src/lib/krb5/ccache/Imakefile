#define IHaveSubdirs
#define PassCDebugFlags

        SUBDIRS = file stdio

MakeSubdirs($(SUBDIRS))

NormalLibraryObjectRule()

OBJS=	ccbase.o \
	ccdefault.o \
	ccdefops.o

SRCS=	ccbase.c \
	ccdefault.c \
	ccdefops.c

OtherdirLibraryTarget($(TOP)/lib,krb5,$(OBJS))
