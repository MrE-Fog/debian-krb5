BUILDTOP=@BUILDTOP@
SUBDIRS = @SUBDIRS@

srcdir = @srcdir@
VPATH = @srcdir@

DEFS = @DEFS@
CC = @CC@
CCOPTS = @CCOPTS@
LIBS = @LIBS@
 
CFLAGS = $(CCOPTS) $(DEFS)
LDFLAGS = -g

RM = rm -f
CP = cp

SRCTOP = $(srcdir)/$(BUILDTOP)

all:: $(OBJS)

OBJS=	ccbase.o \
	ccdefault.o \
	ccdefops.o

SRCS=	$(srcdir)/ccbase.c \
	$(srcdir)/ccdefault.c \
	$(srcdir)/ccdefops.c

