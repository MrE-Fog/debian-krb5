CFLAGS = $(CCOPTS) $(DEFS) -I$(srcdir)$(S)file -I$(srcdir)$(S)stdio

##DOSBUILDTOP = ..\..\..
##DOSLIBNAME=..\krb5.lib
##DOS!include $(BUILDTOP)\config\windows.in

OBJS=	ccbase.$(OBJEXT) \
	ccdefault.$(OBJEXT) \
	ccdefops.$(OBJEXT)

SRCS=	$(srcdir)/ccbase.c \
	$(srcdir)/ccdefault.c \
	$(srcdir)/ccdefops.c

all:: $(OBJS) all-$(WHAT)

all-unix::
all-mac::
all-windows::
	cd FILE
	@echo Making in ccache\file
	-$(MAKE) -$(MFLAGS) LIBCMD=$(LIBCMD)
	cd ..

clean:: clean-$(WHAT)

clean-unix::
clean-mac:
clean-windows::
	cd FILE
	@echo Making clean in ccache\file
	-$(MAKE) -$(MFLAGS) clean
	cd ..
	@echo Making clean locally
