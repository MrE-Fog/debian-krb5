#define IHaveSubdirs
#define PassCDebugFlags
NormalLibraryObjectRule()
        SUBDIRS = ccache keytab krb rcache free os kdb des crc-32
OBJS =	\
	cryptoconf.o \
	perror.o \
	syslog.o \
	vfprintf.o \
	vsprintf.o
SRCS =	\
	cryptoconf.c \
	perror.c \
	syslog.c \
	vfprintf.c \
	vsprintf.c

MakeSubdirs($(SUBDIRS))

AdditiveLibraryTarget(krb5,$(OBJS))
