#define IHaveSubdirs
#define PassCDebugFlags
NormalLibraryObjectRule()

        SUBDIRS = ccache keytab krb rcache free os kdb des crc-32
#ifdef HasVfprintf
VPOBJS=
VPSRCS=
#else
VPOBJS=vfprintf.o vsprintf.o
VPSRCS=vfprintf.c vsprintf.c
#endif

OBJS =	\
	cryptoconf.o \
	perror.o \
	syslog.o \
	$(VPOBJS)

SRCS =	\
	cryptoconf.c \
	perror.c \
	syslog.c \
	$(VPSRCS)

MakeSubdirs($(SUBDIRS))

AdditiveLibraryTarget(krb5,$(OBJS))
