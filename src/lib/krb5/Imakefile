#define IHaveSubdirs
#define PassCDebugFlags
NormalLibraryObjectRule()
        SUBDIRS = ccache crc-32 kdb keytab krb rcache free os des
OBJS =	\
	cryptoconf.o \
	perror.o \
	syslog.o \
	vfprintf.o \
	vsprintf.o
SRCS =	\
	cryptoconf.c \
	perror.c \
	syslog.c \
	vfprintf.c \
	vsprintf.c

MakeSubdirs($(SUBDIRS))

AdditiveLibraryTarget(krb5,$(OBJS))
