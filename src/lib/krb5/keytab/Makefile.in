CFLAGS = $(CCOPTS) $(DEFS)
LDFLAGS = -g

##DOSBUILDTOP = ..\..\..
##DOSLIBNAME=..\krb5.lib
##DOS!include $(BUILDTOP)\config\windows.in

OBJS=	\
	ktadd.$(OBJEXT)		\
	ktbase.$(OBJEXT)	\
	ktdefault.$(OBJEXT)	\
	ktfr_entry.$(OBJEXT)	\
	ktremove.$(OBJEXT)	\
	read_servi.$(OBJEXT)

SRCS=	\
	$(srcdir)/ktadd.c	\
	$(srcdir)/ktbase.c	\
	$(srcdir)/ktdefault.c	\
	$(srcdir)/ktfr_entry.c	\
	$(srcdir)/ktremove.c	\
	$(srcdir)/read_servi.c

all:: $(OBJS) all-$(WHAT)

all-unix::

all-windows::
	cd file
	@echo Making in keytab\file
	-$(MAKE) -$(MFLAGS) LIBCMD=$(LIBCMD)
	cd ..

clean:: clean-$(WHAT)

clean-unix::

clean-windows::
	cd file
	@echo Making clean in keytab\file
	-$(MAKE) -$(MFLAGS) clean
	cd ..
	@echo Making clean locally

