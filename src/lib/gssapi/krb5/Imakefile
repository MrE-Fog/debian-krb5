	verify.o

SRCS= $(ETSRCS) $(CSRCS)
OBJS= $(ETOBJS) $(COBJS)
HDRS= $(ETHDRS)

EHDRDIR=$(TOP)/include/gssapi

NormalLibraryObjectRule()
ErrorTableObjectRule()

all:: ${SRCS} ${HDRS} includes ${OBJS}

clean::
	$(RM) $(ETHDRS) $(ETSRCS)

SubdirLibraryRule($(OBJS))

depend:: $(ETSRCS)

DependTarget()

SpecialObjectRule(gssapi_krb5_err.o,gssapi_krb5_err.c,)
