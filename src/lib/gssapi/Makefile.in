BUILDTOP=@BUILDTOP@
srcdir = @srcdir@
VPATH = @srcdir@

ARCHIVE = @ARCHIVE@
ARADD = @ARADD@
CC = @CC@
CCOPTS = @CCOPTS@
SUBDIRS = @SUBDIRS@
KRB5ROOT = @KRB5ROOT@

KRB5MANROOT = $(KRB5ROOT)/man
ADMIN_BINDIR = $(KRB5ROOT)/admin
SERVER_BINDIR = $(KRB5ROOT)/sbin
CLIENT_BINDIR = $(KRB5ROOT)/bin
ADMIN_MANDIR = $(KRB5MANROOT)/man8
SERVER_MANDIR = $(KRB5MANROOT)/man8
CLIENT_MANDIR = $(KRB5MANROOT)/man1
FILE_MANDIR = $(KRB5MANROOT)/man5
KRB5_LIBDIR = $(KRB5ROOT)/lib
KRB5_INCDIR = $(KRB5ROOT)/include
KRB5_INCSUBDIRS = \
	$(KRB5_INCDIR)/krb5 \
	$(KRB5_INCDIR)/asn.1 \
	$(KRB5_INCDIR)/kerberosIV


RM = rm -f
CP = cp
RANLIB = @RANLIB@

TST=if test -n "`cat DONE`" ; then

all::

libgssapi_krb5.a: generic/DONE krb5/DONE
	(cd generic; $(TST) $(ARADD) ../$@ `cat DONE` ; fi)
	(cd krb5; $(TST) $(ARADD) ../$@ `cat DONE` ; fi)
	$(RANLIB) $@

install:: libgssapi_krb5.a
	$(RM) $(KRB5_LIBDIR)/libgssapi_krb5.a
	$(CP) libgssapi_krb5.a $(KRB5_LIBDIR)/libgssapi_krb5.a
	$(RANLIB) $(KRB5_LIBDIR)/libgssapi_krb5.a

clean::
	$(RM) libgssapi_krb5.a
