NormalLibraryObjectRule()

OBJS=	cksum.o		\
	des.o		\
	enc_dec.o	\
	key_parity.o	\
	key_sched.o	\
	new_rnd_key.o	\
	pcbc_encrypt.o	\
	quad_cksum.o	\
	random_key.o	\
	read_password.o \
	string_to_key.o	\
	weak_key.o	

SRCS=	cksum.c		\
	des.c		\
	enc_dec.c	\
	key_parity.c	\
	key_sched.c	\
	new_rnd_key.c	\
	pcbc_encrypt.c	\
	quad_cksum.c	\
	random_key.c	\
	read_password.c \
	string_to_key.c	\
	weak_key.c	

#ifdef OS_BSD
DEFINES = $(DESDEFINES) -DBSDUNIX
#else
DEFINES = $(DESDEFINES)
#endif

VERIFYOBJ = verify.o

Krb5LibraryTarget(des425,$(OBJS))


EXT_LIB=          $(TOP)/lib/cryptoconf.o \
		$(TOP)/lib/crc-32/libcrc32.a \
		libdes425.a \
		$(TOP)/lib/des/libdes.a \
		$(OSLIB) $(COMERRLIB)

NormalProgramTarget(verify,$(VERIFYOBJ),libdes425.a,$(EXT_LIB),)

NormalLintTarget($(SRCS))

DependTarget()
