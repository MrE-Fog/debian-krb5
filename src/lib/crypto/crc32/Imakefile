NormalLibraryObjectRule()

OBJS=	crc.o

SRCS=	crc.c crctest.c

CRCTESTOBJ = crctest.o

Krb5LibraryTarget(crc32,$(OBJS))

NormalProgramTarget(crctest,$(CRCTESTOBJ),libcrc32.a,libcrc32.a,)

