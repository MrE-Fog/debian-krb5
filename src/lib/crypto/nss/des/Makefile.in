	f_parity.o 	\
	string2key.o

OBJS=	$(OUTPRE)f_parity.$(OBJEXT) 	\
	$(OUTPRE)des_oldapis.$(OBJEXT) 	\
	$(OUTPRE)string2key.$(OBJEXT)

