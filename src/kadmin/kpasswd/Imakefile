# Imakefile for Kerberos admin server library.

#ifdef Krb4KDCCompat
K4LIB=-l$(DES425LIB)
#else
K4LIB=
#endif

SRCS = \
	networked.c \
	kpasswd.c

OBJS = \
	networked.o \
	kpasswd.o

ErrorTableObjectRule()

all::	kpasswd

NormalProgramTarget(kpasswd,$(OBJS),$(KDBDEPLIB) $(DEPKLIB), \
	$(KDBLIB) $(K4LIB) $(KLIB) ,)
Krb5InstallClientProgram(kpasswd)

clean::

depend:: 

clean::

DependTarget()
